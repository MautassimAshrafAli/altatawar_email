{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.py)",
      "dest": "/api/1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

