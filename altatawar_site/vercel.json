{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/send-email.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [{"src": "/api/(.*)", "dest": "send-email.py"}]
}

